<?xml version="1.0" encoding="utf-8"?>
<layout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    >
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".home_fragment"
        android:background="@drawable/a5">

        <ImageView
            android:id="@+id/bg"
            android:layout_width="wrap_content"
            android:layout_height="100dp"
            android:layout_marginTop="58dp"
            android:clickable="true"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/ic_logo2" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="100dp"
            android:fontFamily="@font/moon"
            android:text="@string/app_name"
            android:textAppearance="@style/TextAppearance.Usaha.Title"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/bg" />

        <ImageButton
            android:id="@+id/produkhome"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_marginTop="120dp"
            android:background="@drawable/bggradient"
            android:clickable="true"
            app:layout_constraintEnd_toStartOf="@+id/buttonpelanggan"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/bg"
            app:srcCompat="@drawable/ic_produkhome" />

        <ImageButton
            android:id="@+id/buttonpelanggan"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="65dp"
            android:background="@drawable/bggradient"
            android:clickable="true"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView5"
            app:srcCompat="@drawable/ic_pelanggan" />

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="59dp"
            android:layout_marginEnd="72dp"
            android:clickable="true"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/penjualanhome"
            app:layout_constraintVertical_bias="0.0"
            app:srcCompat="@drawable/ic_info" />

        <ImageButton
            android:id="@+id/penjualanhome"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_marginTop="25dp"
            android:layout_marginEnd="65dp"
            android:background="@drawable/bggradient"
            android:clickable="true"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/buttonpelanggan"
            app:srcCompat="@drawable/ic_pengeluaran" />

        <ImageButton
            android:id="@+id/supplierrrr"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="24dp"
            android:background="@drawable/bggradient"
            app:layout_constraintEnd_toStartOf="@+id/penjualanhome"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/produkhome"
            app:srcCompat="@drawable/ic_supplier" />

        <ImageButton
            android:id="@+id/pembelianhome"
            android:layout_width="101dp"
            android:layout_height="98dp"
            android:layout_marginStart="72dp"
            android:layout_marginBottom="68dp"
            android:background="@drawable/bggradient"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/imageView6"
            app:layout_constraintHorizontal_bias="0.013"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/supplierrrr"
            app:layout_constraintVertical_bias="1.0"
            app:srcCompat="@drawable/ic_shopping_cart" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</layout>